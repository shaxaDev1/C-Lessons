@model List<Ticket>

@{
    ViewData["Title"] = "Tickets";
}

<h1>Tickets</h1>

@for(var i = 0;i < Model.Count; i++)
{
    var ticketName = $"Ticket{i + 1} {Model[i].CorrectCount}/{Model[i].QuestionsCount}";
    <a class="btn btn-primary mb-1" asp-controller="Examinations" asp-action="Exam" 
    asp-route-ticketId="@Model[i].Id">@ticketName</a> 
    <br>
}