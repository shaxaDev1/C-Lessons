<Page x:Class="AutoSavol.Wpf.Files.ExaminationResultFile"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AutoSavol.Wpf.Files"
      mc:Ignorable="d" 
      d:DesignHeight="700" d:DesignWidth="600"
      Title="ExaminationResultFile" Background="Aqua">

    <Grid>
        <Label Content="Examination Result" HorizontalAlignment="Center" FontSize="30"/>
        <StackPanel Margin="10,60,10,10">
            <Label Content="Question count"  FontSize="20"/>
            <TextBlock Name="QuestionCount" Text="0"  FontSize="20"/>
            <Label Content="Correct Answer"  FontSize="20"/>
            <TextBlock Name="CorrectAnswer" Text="0" FontSize="20" Padding="0,0,0,30"/>
            <Button Click="MenuButtonClick" Content="Menu" Height="35" Width="250"/>
        </StackPanel>
       
    </Grid>
</Page>
